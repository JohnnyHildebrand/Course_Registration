@*
*@
@{
	ViewData["Title"] = "Instructor";
}
<h1>Instructor</h1>

<table class="table table-bordered">
	<thead>
		<tr>
			<th>InstructorId</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Email Address</th>
			<th>Course</th>

		</tr>
	</thead>
	<tbody>
		@foreach (var instructor in Model)
			{
			<tr>
				<th scope="row">@instructor.Id</th>
				<td>@instructor.FirstName</td>
				<td>@instructor.LastName</td>
				<td>@instructor.EmailAddress</td>
				<td>@instructor.Course</td>
			</tr>
			}
	</tbody>
</table>

